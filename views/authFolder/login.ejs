<h1>Log In</h1>
<form action="/auth/login" method="POST">
    <label>Username: 
        <input required name="username">
    </label>
    <label>Password: 
        <input type="password" required name="password">
    </label>
    <button type="submit">Log In</button>
</form>

<% if (typeof errMessage !== "undefined") { %>
   <p><%= errMessage %></p>
    <% } %>
 